PATCH = yes
CONFIGURE = yes
CLEAN = yes

include ../Makefile.rules

configure:
	@true

patch:
	@true

prepare:
	@true

download:
	@true

clean:
	@echo -n " $(PACKAGE)"
	@make -C $(COMPILE_DIR) clean >/dev/null 2>&1
